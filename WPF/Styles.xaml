<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="clr-namespace:XJK.WPF.Converters">

    <converters:BoolConverter x:Key="BoolConverter"></converters:BoolConverter>
    <converters:InverseBoolConverter x:Key="InverseBoolConverter"></converters:InverseBoolConverter>
    
    <SolidColorBrush x:Key="ThemeColor" Color="#FF55A3F3"></SolidColorBrush>
    <SolidColorBrush x:Key="DeepColor" Color="#FF0064B3"></SolidColorBrush>
    <SolidColorBrush x:Key="DisableColor" Color="Gray"></SolidColorBrush>
    <SolidColorBrush x:Key="HoverColor" Color="Red"></SolidColorBrush>

</ResourceDictionary>